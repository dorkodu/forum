{
  "scripts": {
    "docker:build:prod": "docker compose -f docker-compose.prod.yml build",
    "docker:build:dev": "docker compose -f docker-compose.dev.yml build",
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "docker:dev:api": "docker exec -it cherno_dev-cherno_api-1 /bin/sh",
    "docker:dev:web": "docker exec -it cherno_dev-cherno_web-1 /bin/sh",
    "docker:dev:postgres": "docker exec -it cherno_dev-cherno_postgres-1 psql -U postgres",
    "docker:dev:keydb": "docker exec -it cherno_dev-cherno_keydb-1 keydb-cli"
  }
}